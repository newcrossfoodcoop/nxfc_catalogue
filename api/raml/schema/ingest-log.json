{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "type": "object",
    "description": "Ingest Log",
    "properties": {
        "_id": {
            "type": "string",
            "example": "57c4b1ba1abb0114001963c5"
        },
        "__v": "number",
        "created": "date",
        "updated": "date",
        "ingest": {
		    "type": "string",
		    "required": true
	    },
	    "status": {
	        "type": "string",
	        "enum": ["new", "running", "success", "fail"],
	        "default": "new",
	        "required": true
	    }
    }
}
